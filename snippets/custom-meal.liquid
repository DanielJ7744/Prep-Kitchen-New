 {%-assign customMealsJson = customMeals | strip_newlines -%}
 <script>
   var customMealsJson = JSON.parse('{{customMealsJson}}');
   var caloriesInfo = [];
   caloriesInfo['protine'] = [];                                 
   caloriesInfo['carb'] = [];
   caloriesInfo['veggie'] = [];
   var totalCalorieInfo = {};                                   
   
   function isEmpty(obj) {
     for(var key in obj) {
       if(obj.hasOwnProperty(key))
         return false;
     }
     return true;
   }
                                    
   function generate_table(){
    // get the reference for the body
     var body = document.getElementById("calories-container");
     body.style.display = 'none';
     body.innerHTML = '';
     // creates a <table> element and a <tbody> element
     var tbl = document.createElement("table");
     var tblHead = document.createElement("thead");
     var tblBody = document.createElement("tbody");
     var tblfoot = document.createElement("tfoot");
     var theaders = ['Item','Calories','Protein(g)','Carbs(g)','Fat(g)','Sodium(mg)'];
     var tbodyval = ['item','calories','protein','carbs','total_fat','sodium'];
     
     if(theaders.length> 0){
       for(var c=0; c < theaders.length; c++){
         var hcell = document.createElement("th");
         var hcellText = document.createTextNode(theaders[c]);
         hcell.appendChild(hcellText);
         // add the row to the end of the table body
         tblHead.appendChild(hcell);
       }  
     }
     var finalInfo = [];
     (caloriesInfo['protine'].length > 0 ? finalInfo.push(caloriesInfo['protine'][0]) : '');
     (caloriesInfo['carb'].length > 0 ? finalInfo.push(caloriesInfo['carb'][0]) : '');
     (caloriesInfo['veggie'].length > 0 ? finalInfo.push(caloriesInfo['veggie'][0]) : '');
     if(finalInfo.length > 0){
       var tfoot = {};
       tfoot.item = 'Total';
       tfoot.calories = 0;
       tfoot.protein = 0;
       tfoot.carbs = 0;
       tfoot.total_fat = 0;
       tfoot.sodium = 0;
       for(var i=0; i < finalInfo.length; i++){
 //         console.log(finalInfo);
          // creates a table row
          var row = document.createElement("tr");
          for (var j = 0; j < 6; j++) {
            // Create a <td> element and a text node, make the text
            // node the contents of the <td>, and put the <td> at
            // the end of the table row
            var cell = document.createElement("td");
            if(tbodyval[j] == 'calories' || tbodyval[j] == 'protein' || tbodyval[j] == 'carbs' || tbodyval[j] == 'total_fat' || tbodyval[j] == 'sodium'){
              var cellText = document.createTextNode((parseFloat(finalInfo[i][tbodyval[j]]) * parseInt(finalInfo[i]['oz'])).toFixed(2)); 
              var cellVal = (parseFloat(finalInfo[i][tbodyval[j]]) * parseInt(finalInfo[i]['oz'])).toFixed(2);
              switch(tbodyval[j]){
                case 'calories':
                 tfoot.calories = parseFloat(tfoot.calories)+parseFloat(cellVal);
                 break;
                case 'protein':
                 tfoot.protein = parseFloat(tfoot.protein)+parseFloat(cellVal);
                 break;
                case 'carbs':
                 tfoot.carbs = parseFloat(tfoot.carbs)+parseFloat(cellVal);
                 break;
                case 'total_fat':
                 tfoot.total_fat = parseFloat(tfoot.total_fat)+parseFloat(cellVal);
                 break;
                case 'sodium':
                 tfoot.sodium = parseFloat(tfoot.sodium)+parseFloat(cellVal);
                 break;  
              }
            }else{
              var cellText = document.createTextNode(finalInfo[i][tbodyval[j]]);
            } 
            cell.appendChild(cellText);
            row.appendChild(cell);
          }
 
         // add the row to the end of the table body
         tblBody.appendChild(row);
       }
       // table footer for grand
       var tfootrow = document.createElement("tr");
       for (var key in tfoot) {
         var tcell = document.createElement("th");
         var tcellText = document.createTextNode((isNaN(tfoot[key]) ? tfoot[key] : tfoot[key].toFixed(2)));
         tcell.appendChild(tcellText);
         tfootrow.appendChild(tcell);
       }
       
       
       if(isEmpty(tfoot)) {
         totalCalorieInfo = {};
       }else{
         totalCalorieInfo = tfoot;
         if($('#calorie-info').length > 0){
           $('#calorie-info').val('');
           $('#calorie-info').val("CALORIES: "+totalCalorieInfo.calories.toFixed(1)+" / PROTEIN: "+totalCalorieInfo.protein.toFixed(1)+"g / CARBS: "+totalCalorieInfo.carbs.toFixed(2)+"g FAT: "+totalCalorieInfo.total_fat.toFixed(1)+"g");
         }
       } 
       
       
       //add footer row
       tblfoot.appendChild(tfootrow);
       
 //       console.log(tfoot);
       // put the <thead> in the <table>
       tbl.appendChild(tblHead);
       // put the <tbody> in the <table>
       tbl.appendChild(tblBody);
       //add footer
       tbl.appendChild(tblfoot);
       // appends <table> into <body>
       body.appendChild(tbl);
       body.style.display = 'block';
     }
   }
  
 </script>